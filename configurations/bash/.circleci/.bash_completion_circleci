_circleci_config()
{
    last_command="circleci_config"

    command_aliases=()

    commands=()
    commands+=("pack")
    commands+=("process")
    commands+=("validate")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("check")
        aliashash["check"]="validate"
    fi

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--config-api-host=")
    two_word_flags+=("--config-api-host")
    flags+=("--host=")
    two_word_flags+=("--host")
    flags+=("--skip-update-check")
    flags+=("--token=")
    two_word_flags+=("--token")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

